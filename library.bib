}

@article{Morano2013,
  ids = {morano},
  title = {The Burgeoning {{HIV}}/{{HCV}} Syndemic in the Urban {{Northeast}}: {{HCV}}, {{HIV}}, and {{HIV}}/{{HCV}} Coinfection in an Urban Setting},
  author = {Morano, Jamie P and Gibson, Britton A and Altice, Frederick L},
  year = {2013},
  month = may,
  volume = {8},
  pages = {e64321-e64321},
  publisher = {{Public Library of Science}},
  address = {{United States}},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0064321},
  abstract = {INTRODUCTION: Despite recommendations for generation-based HCV and once lifetime HIV screening, thousands of individuals in the U.S. still remain untested and undiagnosed. This cross-sectional study examines the correlates of HCV and HIV monoinfection and HIV/HCV coinfection in an urban Northeast setting. METHODS: Utilizing an electronic database from a mobile medical clinic in New Haven, CT from January 2003 to July 2011, 8,311 individuals underwent structured health assessment and screening for HIV and HCV. RESULTS: HIV [N{$\mkern1mu$}={$\mkern1mu$}601 (8.0\%)] and HCV [N{$\mkern1mu$}={$\mkern1mu$}753 (10.1\%)] infection were identified, and 197 (26.1\%) of the 753 with HCV were coinfected with HIV. Both monoinfection and coinfection status were independently correlated with crack cocaine use and increasing age. HIV/HCV coinfection was correlated with men having sex with men (MSM) (AOR{$\mkern1mu$}={$\mkern1mu$}38.53, p{$<$}0.0080), shooting gallery use (AOR{$\mkern1mu$}={$\mkern1mu$}3.06, p{$<$}0.0070), and not completing high school (AOR{$\mkern1mu$}={$\mkern1mu$}2.51, p{$<$}0.0370). HCV monoinfection correlated with health insurance (AOR{$\mkern1mu$}={$\mkern1mu$}2.16, p{$<$}0.0020), domestic violence (AOR{$\mkern1mu$}={$\mkern1mu$}1.99, p{$<$}0.0070), and being Hispanic (AOR{$\mkern1mu$}={$\mkern1mu$}2.63, p{$<$}0.0001), while HIV monoinfection correlated with having had syphilis (AOR{$\mkern1mu$}={$\mkern1mu$}2.66, p{$<$}0.0001) and being Black (AOR{$\mkern1mu$}={$\mkern1mu$}1.73, p{$\mkern1mu$}={$\mkern1mu$}0.0010). CONCLUSIONS: Though HIV and HCV share common transmission risk behaviors, independent correlates with viral infection status in an urban Northeast setting are distinct and have important implications for surveillance, healthcare delivery, disease prevention, and clinical care.},
  journal = {PloS one},
  keywords = {A lire},
  language = {eng},
  number = {5}
}

@article{Morris2019,
