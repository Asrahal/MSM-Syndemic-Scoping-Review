  title = {{{LGB}}-Affirmative Cognitive-Behavioral Therapy for Young Adult Gay and Bisexual Men: {{A}} Randomized Controlled Trial of a Transdiagnostic Minority Stress Approach.},
  author = {Pachankis, John E and Hatzenbuehler, Mark L and Rendina, H Jonathon and Safren, Steven A and Parsons, Jeffrey T},
  year = {2015},
  month = oct,
  volume = {83},
  pages = {875--889},
  publisher = {{American Psychological Association,US}},
  address = {{US}},
  issn = {1939-2117},
  doi = {10.1037/ccp0000037},
  abstract = {Objectives-We tested the preliminary efficacy of a transdiagnostic cognitive behavioral treatment adapted to improve depression, anxiety, and co-occurring health risks (i.e., alcohol use, sexual compulsivity, condomless sex) among young adult gay and bisexual men. Treatment adaptations focused on reducing minority stress processes that underlie sexual orientation-related mental health disparities. Method-Young gay and bisexual men (n=63; M age=25.94) were randomized to immediate treatment or a three-month waitlist. At baseline, 3-month, and 6-month assessments, participants completed self-reports of mental health and minority stress and an interview of past-90-day risk behavior. Results-Compared to waitlist, treatment significantly reduced depressive symptoms (b=-2.43, 95\% CI: -4.90, 0.35, p{$<$}0.001), alcohol use problems (b =-3.79, 95\% CI: -5.94, -1.64, p{$<$}0.001), sexual compulsivity (b =-5.09, 95\% CI: -8.78, -1.40, p{$<$}0.001), and past-90-day condomless sex with casual partners (b =-1.09, 95\% CI: -1.80, -0.37, p{$<$}0.001), and improved condom use self-efficacy (b =10.08, 95\% CI: 3.86, 16.30, p{$<$}0.001). The treatment yielded moderate and marginally significant greater improvements than waitlist in anxiety symptoms (b =-2.14, 95\% CI: -4.61, 0.34, p=0.09) and past-90-day heavy drinking (b =-0.32, 95\% CI: -0.71, 0.07, p=0.09). Effects were generally maintained at follow-up. Minority stress processes showed small improvements in the expected direction.},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\DJ44NPEN\\Pachankis et al. - LGB-affirmative cognitive-behavioral therapy for y.pdf;C\:\\Users\\maxen\\Zotero\\storage\\I4TPGF6F\\Pachankis et al. - 2015 - LGB-affirmative cognitive-behavioral therapy for young adult gay and bisexual men A randomized controlled tria.pdf},
  journal = {Journal of Consulting and Clinical Psychology},
  keywords = {A lire,Minority stress,MSM},
  number = {5}
}

@article{Pachankis2015a,
