@article{Bourey2018,
  title = {Syndemic {{Vulnerability}} and {{Condomless Sex Among Incarcerated Men}} in {{Mexico City}}: {{A Latent Class Analysis}}},
  author = {Bourey, Christine and Stephenson, Rob and {Bautista-Arredondo}, Sergio},
  year = {2018},
