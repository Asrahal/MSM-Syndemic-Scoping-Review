@article{hsieh,
  title = {Social {{Relationships}} and {{Loneliness}} in {{Late Adulthood}}: {{Disparities}} by {{Sexual Orientation}}},
  author = {Hsieh, Ning and Liu, Hui},
  doi = {10.1111/jomf.12681},
  abstract = {{$<$}section class="article-section\_\_content"{$>$} {$<$}h3 class="article-section\_\_sub-title section1"{$>$} Objective{$<$}/h3{$>$} {$<$}p{$>$}This is the first national study to examine disparities in loneliness and social relationships by sexual orientation in late adulthood in the United States.{$<$}/p{$>$} {$<$}/section{$>$} {$<$}section class="article-section\_\_content"{$>$} {$<$}h3 class="article-section\_\_sub-title section1"{$>$} Background{$<$}/h3{$>$} {$<$}p{$>$}Prior studies have shown that lesbian, gay, and bisexual (LGB) individuals often struggle with social relationships across the life course, likely because of stigma related to sexual orientation. However, little is known about whether loneliness is more prevalent among LGB people than among other groups in late adulthood, and if so, which relationships contribute to the loneliness  gap.{$<$}/p{$>$} {$<$}/section{$>$} {$<$}section class="article-section\_\_content"{$>$} {$<$}h3 class="article-section\_\_sub-title section1"{$>$} Method{$<$}/h3{$>$} {$<$}p{$>$}We analyzed data from a nationally representative sample of older adults from the 2015\textendash 2016 National Social Life, Health, and Aging Project (\emph{N}~=~3,567) to examine the disparity in loneliness by sexual orientation and identify links between this disparity and multiple dimensions of social relationships, including partner, family, friend, and community relationships.             {$<$}/p{$>$} {$<$}/section{$>$} {$<$}section class="article-section\_\_content"{$>$} {$<$}h3 class="article-section\_\_sub-title section1"{$>$} Results{$<$}/h3{$>$} {$<$}p{$>$}Older LGB adults were significantly lonelier than their heterosexual counterparts, primarily due to a lower likelihood of having a partner and, to a lesser extent, lower levels of family support and greater friend strain. While they were also disadvantaged in the size of close family and frequency of community participation, these factors were less relevant to their loneliness. Overall, the conventionally defined inner layers of relationships (partnership and family) contributed more to the loneliness disparity than the outer layers of relationships (friends and community).{$<$}/p{$>$} {$<$}/section{$>$} {$<$}section class="article-section\_\_content"{$>$} {$<$}h3 class="article-section\_\_sub-title section1"{$>$} Conclusion{$<$}/h3{$>$} {$<$}p{$>$}These findings suggest that strengthening the partnerships and family relationships of sexual minorities is essential to reducing the loneliness  gap.{$<$}/p{$>$} {$<$}/section{$>$}},
  journal = {Journal of Marriage and Family},
  keywords = {A lire,Aging,Loneliness,MSM,WSW}
}

