  title = {Endocarditis Associated with Vertebral Osteomyelitis and Septic Arthritis of the Axial Skeleton},
  author = {Murillo, Oscar and Grau, Imma and {Gomez-Junyent}, Joan and Cabrera, Celina and Ribera, Alba and Tubau, Fe and Pe{\~n}a, Carmen and Ariza, Javier and Pallares, Roman},
