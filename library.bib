}

@article{notini,
  title = {"{{No One Stays Just}} on {{Blockers Forever}}": {{Clinicians}}' {{Divergent Views}} and {{Practices Regarding Puberty Suppression}} for {{Nonbinary Young People}}},
  author = {Notini, Lauren and Pang, Ken C. and Telfer, Michelle and McDougall, Rosalind},
  doi = {10.1016/j.jadohealth.2020.09.028},
  abstract = {{$<$}h3 class="u-h4 u-margin-m-top u-margin-xs-bottom"{$>$}Purpose{$<$}/h3{$><$}p{$>$}Approximately half of transgender or gender diverse youth identify as gender nonbinary (not entirely or exclusively male or female). This study aimed to explore the views and practices of Australian clinicians working with transgender or gender diverse young people regarding puberty suppression for nonbinary youth, including requests for ongoing puberty suppression into adulthood.{$<$}/p{$><$}h3 class="u-h4 u-margin-m-top u-margin-xs-bottom"{$>$}Methods{$<$}/h3{$><$}p{$>$}Individual semistructured key informant interviews were conducted with 14 clinicians (medical and mental health) in Australia to explore their practices, views, and decision-making regarding puberty suppression for nonbinary youth. Transcripts were analyzed using inductive content analysis.{$<$}/p{$><$}h3 class="u-h4 u-margin-m-top u-margin-xs-bottom"{$>$}Results{$<$}/h3{$><$}p{$>$}Requests for puberty suppression from nonbinary young people are occurring, with 12 of 14 clinicians reporting they have received such requests. Although all clinicians were prepared to support short-term puberty suppression for nonbinary youth, clinicians diverged in their preparedness to support long-term puberty suppression. Clinicians expressed concerns in relation to risks, resource allocation, and medicalization. Some clinicians required nonbinary young people to choose a masculinizing or feminizing puberty. Some clinicians also put forward arguments in favor of long-term puberty suppression, including potential psychosocial harm of discontinuing puberty suppression, respect for autonomy, equity, and harm minimization. Clinicians also highlighted practical challenges of obtaining long-term puberty suppression in the adult setting.{$<$}/p{$><$}h3 class="u-h4 u-margin-m-top u-margin-xs-bottom"{$>$}Conclusions{$<$}/h3{$><$}p{$>$}The decision of whether to provide puberty suppression, particularly long-term, to a nonbinary young person is both ethically and clinically complex. There is currently a diversity of practice in this area, and evidence-based ethical guidelines and outcome data could promote more informed decision-making and support clinicians working in this complex area.{$<$}/p{$>$}},
  journal = {Journal of Adolescent Health},
  keywords = {Researcher App}
}

@article{ocleirigh2018,
  title = {Co-Occurring Psychosocial Problems Predict {{HIV}} Status and Increased Health Care Costs and Utilization among Sexual Minority Men.},
  author = {O'Cleirigh, Conall and Pantalone, David W and Batchelder, Abigail W and Hatzenbuehler, Mark L and Marquez, Samantha M and Grasso, Chris and Safren, Steven A and Mayer, Kenneth H},
  year = {2018},
  volume = {41},
  pages = {450--457},
  address = {{United States}},
  issn = {1573-3521,0160-7715},
  doi = {10.1007/s10865-018-9913-z},
  abstract = {Sexual orientation related health disparities are well documented. Sexual minority men appear to be at risk for mental health problems due to the stress they experience in establishing and maintaining a minority sexual identity. These mental health issues may combine synergistically and lead to higher medical costs to society. We examine whether sexual minority specific syndemic indicators were associated with higher health care costs, health care utilization, or the risk of being HIV-infected. Health care consumers at a community health center (N = 1211) completed a brief screening questionnaire collected over 12 months. Self-reported data were linked with participants' clinical billing records. Adjusted logistic regression models identified that four syndemic indicators (suicidality, substance use, childhood sexual abuse, and intimate partner violence) were each significantly related to each other. Multiple syndemics significantly predicted higher medical care utilization and cost, and were associated with 2.5 times the risk of being HIV-infected (OR 2.49, 95\% CI 1.45-4.25). Syndemic indicators did not significantly predict the number of mental health visits or costs per patient. These results confirm and extend earlier findings by relating syndemics to health services use and costs for sexual minority men.},
  annotation = {O'Cleirigh, Conall. Department of Psychiatry, Massachusetts General Hospital/Harvard Medical School, 1 Bowdoin Square, 7th Floor, Boston, MA, 02114, USA. cocleirigh@mgh.harvard.edu.,O'Cleirigh, Conall. The Fenway Institute, Fenway Health, Boston, MA, USA. cocleirigh@mgh.harvard.edu.,Pantalone, David W. The Fenway Institute, Fenway Health, Boston, MA, USA.,Pantalone, David W. Suffolk University, Boston, MA, USA.,Batchelder, Abigail W. Department of Psychiatry, Massachusetts General Hospital/Harvard Medical School, 1 Bowdoin Square, 7th Floor, Boston, MA, 02114, USA.,Batchelder, Abigail W. The Fenway Institute, Fenway Health, Boston, MA, USA.,Hatzenbuehler, Mark L. Mailman School of Public Health, Columbia University, New York, NY, USA.,Marquez, Samantha M. The Fenway Institute, Fenway Health, Boston, MA, USA.,Grasso, Chris. The Fenway Institute, Fenway Health, Boston, MA, USA.,Safren, Steven A. The Fenway Institute, Fenway Health, Boston, MA, USA.,Safren, Steven A. University of Miami, Coral Gables, FL, USA.,Mayer, Kenneth H. The Fenway Institute, Fenway Health, Boston, MA, USA.,Mayer, Kenneth H. Beth Israel Deaconess Medical Center/Harvard Medical School, Boston, MA, USA.},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\D96WLAVL\\O'Cleirigh et al. - 2018 - Co-occurring psychosocial problems predict HIV sta.pdf},
  journal = {J Behav Med},
  keywords = {Childhood sexual abuse,Healthcare access,IPV,MSM,Substance use,Suicide,Syndemic}
}

@article{ocleirigha,
  title = {Cognitive {{Behavioral Therapy}} for {{Trauma}} and {{Self}}-{{Care}} ({{CBT}}-{{TSC}}) in Men Who Have Sex with Men with a History of Childhood Sexual Abuse: {{A}} Randomized Controlled Trial.},
  author = {O'Cleirigh, Conall and Safren, Steven A and Taylor, S. Wade and Goshe, Brett M and Bedoya, C. Andres and Marquez, Samantha M and Boroughs, Michael S and Shipherd, Jillian C},
