@article{Vansintejan2016,
  title = {The State of Sexual Health of {{Belgian WSW}} Anno 2015},
  author = {Vansintejan, J. and {op de Beke}, E. and Devroey, D.},
  year = {2016},
  volume = {13},
  pages = {S99-S100},
  publisher = {{Elsevier Inc}},
  issn = {17436095},
  doi = {10.1016/j.jsxm.2016.03.067},
  abstract = {Objective: This study aims to collect data on the sexual health of Belgian women who have sex with women (WSW), a frequently omitted minority, for which a lack of national objective data exists. Methods: An internet-based survey in Dutch and French on sexual behaviour and sexual transmitted diseases (STDs) was administered during the months of February and March 2015. The survey assessed prevalence of STD, the use, attitude towards and knowledge of safe sex, frequency of Pap smears and quality of relationships with caregivers. Results: Among the 209 Belgian participants, 34,9\% declared to have at least one STD. The most reported STDs were candida (16,3\%) and bacterial vaginosis (13,9\%). The occurrence of STD with a serious impact on health were minimal (Chlamydia 1.9\%, herpes 3\%) to none (hepatitis, hiv). Sex with a man and a higher number of sex partners were found to be associated risk factors for a STD. Only 10 - 20 \% of WSW reported to have safe sexual encounters, highly depending on which sexual act was performed. Only 34.3\% of WSW had a Pap smear done in a 3 year interval and 37,3\% of the {$>$}25 years women admitted they did not have one yet. Relationships with caregivers were positively appreciated, although most women stated they received little or no education about safe sex their relationships. Conclusion: With a relatively high incidence of STDs, but none with serious impact on general health, the lack of safe sex use in this population seems to be a debatable problem. These findings highlight the need for more information and education about female homosexual eroticism and safe sex.},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\3PC8SN3F\\Vansintejan, op de Beke, Devroey - 2016 - The state of sexual health of Belgian WSW anno 2015.pdf},
  journal = {The Journal of Sexual Medicine},
  number = {5}
}

