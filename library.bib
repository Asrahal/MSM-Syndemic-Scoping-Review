  journal = {Drug Alcohol Rev.},
  keywords = {Black MSM,Healthcare access,Incarceration,Latino MSM,MSM,Syndemic,Unemployment,Unstable housing},
  language = {eng},
  number = {2}
}

@article{R2017,
