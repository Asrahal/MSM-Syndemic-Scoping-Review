  journal = {Journal of psychoactive drugs},
  keywords = {A lire},
  language = {eng},
  number = {4}
}

@article{Kurtz2012,
  ids = {kurtz},
  title = {Resilience, Syndemic Factors, and Serosorting Behaviors among {{HIV}}-Positive and {{HIV}}-Negative Substance-Using {{MSM}}},
  author = {Kurtz, Steven P and Buttram, Mance E and Surratt, Hilary L and Stall, Ronald D},
  year = {2012},
  month = jun,
  volume = {24},
  pages = {193--205},
  address = {{United States}},
  issn = {1943-2755},
  doi = {10.1521/aeap.2012.24.3.193},
  abstract = {Serosorting is commonly employed by MSM to reduce HIV risk. We hypothesize that MSM perceive serosorting to be effective, and that serosorting is predicted by resilience and inversely related to syndemic characteristics. Surveys included 504 substance-using MSM. Logistic regression models examined syndemic and resilience predictors of serosorting, separately by serostatus. For HIV-positive men, positive coping behaviors (P = .015) and coping self-efficacy (P = .014) predicted higher odds, and cognitive escape behaviors (P = .003) lower odds, of serosorting. For HIV-negative men, social engagement (P = .03) and coping self-efficacy (P = .01) predicted higher odds, and severe mental distress (P = .001), victimization history (P = .007) and cognitive escape behaviors (P = .006) lower odds, of serosorting. HIV-negative serosorters reported lower perceptions of risk for infection than non-serosorters (P {$<$} .000). Although high risk HIV-negative men may perceive serosorting to be effective, their high rates of UAI and partner change render this an ineffective risk reduction approach. Relevant public health messages are urgently needed.},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\U28HMSVU\\Kurtz et al. - 2012 - Resilience, syndemic factors, and serosorting beha.pdf},
  journal = {AIDS Educ Prev},
  keywords = {Anxiety,Depression,MSM,Substance Use Disorder,Syndemic,Violence},
  language = {eng},
  number = {3}
}

@article{lachowsky,
  title = {Lifetime {{Doctor}}-{{Diagnosed Mental Health Conditions}} and {{Current Substance Use Among Gay}} and {{Bisexual Men Living}} in {{Vancouver}}, {{Canada}}.},
  author = {Lachowsky, Nathan J and Dulai, Joshun J S and Cui, Zishan and Sereda, Paul and Rich, Ashleigh and Patterson, Thomas L and Corneil, Trevor T and Montaner, Julio S G and Roth, Eric A and Hogg, Robert S and Moore, David M},
  pages = {785},
  address = {{England}},
  issn = {1532-2491,1082-6084},
  doi = {10.1080/10826084.2016.1264965},
  abstract = {BACKGROUND: Studies have found that gay, bisexual, and other men who have sex with men (GBM) have higher rates of mental health conditions and substance use than heterosexual men, but are limited by issues of representativeness., OBJECTIVES: To determine the prevalence and correlates of mental health disorders among GBM in Metro Vancouver, Canada., METHODS: From 2012 to 2014, the Momentum Health Study recruited GBM ({$>$}=16 years) via respondent-driven sampling (RDS) to estimate population parameters. Computer-assisted self-interviews (CASI) collected demographic, psychosocial, and behavioral information, while nurse-administered structured interviews asked about mental health diagnoses and treatment. Multivariate logistic regression using manual backward selection was used to identify covariates for any lifetime doctor diagnosed: (1) alcohol/substance use disorder and (2) any other mental health disorder., RESULTS: Of 719 participants, 17.4\% reported a substance use disorder and 35.2\% reported any other mental health disorder; 24.0\% of all GBM were currently receiving treatment. A lifetime substance use disorder diagnosis was negatively associated with being a student (AOR = 0.52, 95\% CI [confidence interval]: 0.27-0.99) and an annual income {$>$}=\$30,000 CAD (AOR = 0.38, 95\% CI: 0.21-0.67) and positively associated with HIV-positive serostatus (AOR = 2.54, 95\% CI: 1.63-3.96), recent crystal methamphetamine use (AOR = 2.73, 95\% CI: 1.69-4.40) and recent heroin use (AOR = 5.59, 95\% CI: 2.39-13.12). Any other lifetime mental health disorder diagnosis was negatively associated with self-identifying as Latin American (AOR = 0.25, 95\% CI: 0.08-0.81), being a refugee or visa holder (AOR = 0.18, 95\% CI: 0.05-0.65), and living outside Vancouver (AOR = 0.52, 95\% CI: 0.33-0.82), and positively associated with abnormal anxiety symptomology scores (AOR = 3.05, 95\% CI: 2.06-4.51)., CONCLUSIONS: Mental health conditions and substance use, which have important implications for clinical and public health practice, were highly prevalent and co-occurring.},
  annotation = {Lachowsky, Nathan J. a School of Public Health \& Social Policy, University of Victoria, Victoria, Canada.,Lachowsky, Nathan J. b Centre for Addictions Research British Columbia, Victoria, Canada.,Dulai, Joshun J S. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Cui, Zishan. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Sereda, Paul. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Rich, Ashleigh. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Patterson, Thomas L. d Department of Psychiatry, University of California, San Diego, La Jolla, California, USA.,Corneil, Trevor T. e School of Population and Public Health, University of British Columbia, Vancouver, Canada.,Montaner, Julio S G. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Montaner, Julio S G. f Faculty of Medicine, University of British Columbia, Vancouver, Canada.,Roth, Eric A. a School of Public Health \& Social Policy, University of Victoria, Victoria, Canada.,Roth, Eric A. b Centre for Addictions Research British Columbia, Victoria, Canada.,Hogg, Robert S. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Hogg, Robert S. g Faculty of Health Sciences, Simon Fraser University, Burnaby, Canada.,Moore, David M. c British Columbia Centre for Excellence in HIV/AIDS, Vancouver, Canada.,Moore, David M. f Faculty of Medicine, University of British Columbia, Vancouver, Canada.},
  journal = {Substance use \& misuse},
  keywords = {A lire},
