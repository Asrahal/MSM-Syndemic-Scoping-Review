@article{Hammoud2018,
  title = {The New {{MTV}} Generation: {{Using}} Methamphetamine, {{Truvada}}\texttrademark{} and {{Viagra}}\texttrademark{} to Enhance Sex and Stay Safe},
  author = {Hammoud, Mohamed A. and Vaccher, Stefanie and Jin, Fengyi and Bourne, Adam and Haire, Bridget and Maher, Lisa and Lea, Toby and Prestage, Garrett},
  year = {2018},
  month = may,
  volume = {55},
  pages = {197--204},
  publisher = {{Elsevier B.V.}},
  issn = {18734758},
  doi = {10.1016/j.drugpo.2018.02.021},
  abstract = {Introduction: Gay and bisexual men (GBM) often use illicit drugs to enhance sexual pleasure, commonly referred to as `chemsex' or `party n play'. In particular, the use of methamphetamine and Viagra\texttrademark{} and other erectile dysfunction medications, both together and separately are strongly predictive of subsequent HIV infection. Truvada\texttrademark{} as pre-exposure prophylaxis (PrEP), virtually eliminates HIV transmission during condomless anal intercourse (CLAI). HIV-negative GBM in intensive sex partying networks may be adding PrEP to their drug regimen to actively reduce the possibility of HIV transmission during chemsex. Aim: We describe the prevalence and context of concurrent use of methamphetamine, Truvada\texttrademark{} (or its generic formulations), and Viagra\texttrademark{} or other erectile dysfunction medication (collectively, MTV). Method: The Following Lives Undergoing Change study is an online prospective observational study of licit and illicit drug use among Australian GBM. Between January and July 2017, 1831 GBM provided details about their use of MTV. Binary logistic multiple regression analysis were used to estimate adjusted odds ratios (aOR) and associated 95\% confidence intervals (95\%CI). Results: Concurrent MTV use was reported by 6.0\% of participants; 3.1\% used methamphetamine and Viagra\texttrademark{} or other erectile dysfunction medication (`MV only') and 11.2\% used Truvada\texttrademark{} as PrEP (`T only'). In multivariate analysis, compared to use of `MV only' MTV was independently associated with CLAI with casual partners (aOR = 6.78;95\%CI = 1.42\textendash 32.34) and `fuckbuddies' (aOR = 3.47;95\%CI = 1.41\textendash 8.56) in the previous six months. Compared to use of `T only' MTV was independently associated with being older (aOR = 3.95;95\%CI = 1.55\textendash 10.03) and engaging in group sex (aOR = 3.31;95\%CI = 1.82\textendash 6.00). Greater social engagement with other gay men (aOR = 1.44;95\%CI = 1.18\textendash 1.76) and having more sexual partners (aOR = 2.30;95\%CI = 1.10\textendash 4.82) were independently associated with use of MTV compared to use of `MV only' or `T only'. Conclusion: GBM in intensive sex partying networks are increasingly adding PrEP alongside other drugs they use to enhance sexual experiences. Interventions that promote the use of PrEP during chemsex could mitigate HIV risk.},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\5EKQ78AC\\Hammoud et al. - 2018 - The new MTV generation Using methamphetamine, Tru.pdf},
  journal = {International Journal of Drug Policy},
  keywords = {MSM}
}

@article{Hammoud2018a,
  title = {Intensive Sex Partying with Gamma-Hydroxybutyrate: Factors Associated with Using Gamma-Hydroxybutyrate for Chemsex among {{Australian}} Gay and Bisexual Men \textendash{} Results from the {{Flux Study}}},
  author = {Hammoud, Mohamed A. and Bourne, Adam and Maher, Lisa and Jin, Fengyi and Haire, Bridget and Lea, Toby and Degenhardt, Louisa and Grierson, Jeffrey and Prestage, Garrett},
  year = {2018},
  volume = {15},
  pages = {123},
  issn = {1448-5028},
  doi = {10.1071/SH17146},
  abstract = {Background Gamma-hydroxybutyrate (GHB) use among gay and bisexual men (GBM) has increased in recent years. It is commonly cited as a sexual-enhancement drug. There is, however, little evidence for factors associated with GHB use or the consequences of its use among GBM. Aim: Factors associated with GHB use, its relationship to sexual risk behaviour, and the contexts, consequences, and motivations for its use were examined. Methods: The Following Lives Undergoing Change (Flux) Study is an online prospective observational study of Australian GBM. At baseline, a total of 3190 GBM provided details about their use of GHB. Data on frequency, methods, pleasures and consequences of their drug use, alongside key demographic variables were collected. Results: Mean age was 35.0 years. One in five men (19.5\%) had a history of GHB use and 5.4\% reported use within the past 6 months, with 2.7\% having used it monthly or more frequently. Overdose had been experienced by 14.7\%, this was more common among men who used GHB at least monthly. Being HIV-positive, having more gay friends, greater social engagement with gay men who use drugs, a greater number of sexual partners, group sex, and condomless anal intercourse with casual partners were independently associated with GHB use in the past 6 months. Greater social engagement with gay men who use drugs and group sex were independently associated with at least monthly use. More frequent GHB use was independently associated with experiencing overdose among GHB users. Conclusion: Most men used GHB infrequently and it was often used explicitly to enhance sexual experiences, often in the context of intensive sex partying. Men who used GHB frequently, were at greater risk of overdose and other negative health outcomes. GHB use should be considered alongside other drugs that have been implicated in sexual risk behaviour and HIV transmission. Harm-reduction interventions need to consider the particular impact of frequent GHB use.},
  journal = {Sexual Health},
  keywords = {A lire,Bisexuals,Chemsex,GHB,MSM,MSMW,Substance use},
  number = {2}
}

@phdthesis{hampton,
  title = {Measurement Model Exploring a Syndemic among {{HIV}}-Positive Gay, Bisexual, and Other {{MSM}} over 50 in {{New York}} City.},
  author = {Hampton, Melvin C},
  address = {{US}},
  abstract = {After more than 30 years since the beginning of the HIV epidemic in the U.S., gay, bisexual, and other men who have sex with men (MSM) continue to suffer from disproportionate rates of HIV. In addition, it is predicted that in the near future over 50\% of individuals infected with HIV in the U.S. will be over the age of 50, with gay, bisexual, and other MSM making up the majority of this population. Given this, more research is need in order to better understand the experiences of HIV-positive gay, bisexual, and other MSM over 50 as they transition into later adulthood. This dissertation attempted to address this gap in scholarship via the use of syndemic theory. Specifically, structural equation modeling techniques were used in order to assess for a possible syndemic among HIV-positive gay, bisexual, and other MSM over 50 in New York City. This syndemic was theorized to be comprised of the epidemics of mental health burden, substance use, and sexual risk-taking. Three first-order latent factors were found for mental health burden, substance use, and sexual risk-taking respectively. An attempt to construct a second-order latent factor, which was constitutive of the three first-order latent factors, and theorized to represent an overall syndemic among the men sampled, was unsuccessful. Further analyses attempted to assess the impact of lifetime reports of mental health burden, substance use, and sexual risk-taking on the presence of the three first-order latent factors at the time of assessment. In addition, the relations between the potential protective factors of religiosity, spirituality, and existential well-being and the three first-order latent factors of mental health burden, substance use, and sexual risk-taking at the time of assessment were assessed. Models assessing for the impact of lifetime reports of burden and risk, as well as for the noted protective factors, provided poor model fit. Further moderation effects were hypothesized; however, these hypotheses remained untested, due to poor model fit. The theoretical, public health, and counseling implications of these findings is discussed. In addition, given the results of this study, the need for further prospective study in order to assess for the potential of a syndemic among HIV-positive gay, bisexual, and other MSM in the U.S. is highlighted. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\6G5QQRIW\\Hampton - Measurement model exploring a syndemic among HIV-p.pdf},
  keywords = {A lire},
  school = {ProQuest Information \& Learning,US}
}

