@article{bellet2018,
  title = {Bereavement {{Outcomes}} as {{Causal Systems}}: {{A Network Analysis}} of the {{Co}}-{{Occurrence}} of {{Complicated Grief}} and {{Posttraumatic Growth}}},
  shorttitle = {Bereavement {{Outcomes}} as {{Causal Systems}}},
  author = {Bellet, Benjamin W. and Jones, Payton J. and Neimeyer, Robert A. and McNally, Richard J.},
  year = {2018},
  month = nov,
  volume = {6},
  pages = {797--809},
  issn = {2167-7026, 2167-7034},
  doi = {10.1177/2167702618777454},
  abstract = {Bereavement can lead to negative outcomes such as complicated grief (CG), but some mourners with symptoms of CG often experience positive sequelae of loss such as posttraumatic growth (PTG) as well. We propose that grief and growth co-occur and change one another because they alternately reinforce and weaken each other at the level of their respective constituent elements. We investigated the structure of a network of CG and PTG elements to elucidate how grief and growth can co-occur within a potentially causal system in bereaved young adults. Challenges to control and identity disturbance ranked as the most highly central symptoms in the CG network; the discovery of a new life path and greater personal strength were similarly central elements of PTG. Finally, the degree of disruption and change in mourners' worldviews emerged as the element that most strongly bridged the two domains, suggesting a pivotal connection between grief and growth.},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\4ZNG63M3\\Bellet et al. - 2018 - Bereavement Outcomes as Causal Systems A Network .pdf},
  journal = {Clinical Psychological Science},
  keywords = {A lire,Network analysis,PTSD},
  language = {en},
  number = {6}
}

