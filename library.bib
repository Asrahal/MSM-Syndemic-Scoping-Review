@article{Mustanski2007,
  ids = {mustanskib},
  title = {Psychosocial Health Problems Increase Risk for {{HIV}} among Urban Young Men Who Have Sex with Men: Preliminary Evidence of a Syndemic in Need of Attention},
  author = {Mustanski, Brian and Garofalo, Robert and Herrick, Amy and Donenberg, Geri},
  year = {2007},
  month = aug,
  volume = {34},
