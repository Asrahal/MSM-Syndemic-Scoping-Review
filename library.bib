@article{martinez,
  title = {Transhealth {{Information Project}}: {{A Peer}}-{{Led HIV Prevention Intervention}} to {{Promote HIV Protection}} for {{Individuals}} of {{Transgender Experience}}.},
  author = {Martinez, Omar and Lopez, Nikki and Woodard, Tatyana and {Rodriguez-Madera}, Sheilla and Icard, Larry},
