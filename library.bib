}

@article{Pachankis2015b,
  title = {The Mental Health of Sexual Minority Adults in and out of the Closet: {{A}} Population-Based Study},
  author = {Pachankis, John E and Cochran, Susan D and Mays, Vickie M},
  year = {2015},
