@article{Zash2018,
  title = {Comparative Safety of Dolutegravir-Based or Efavirenz-Based Antiretroviral Treatment Started during Pregnancy in {{Botswana}}: An Observational Study},
  author = {Zash, Rebecca and Jacobson, Denise L. and Diseko, Modiegi and Mayondi, Gloria and Mmalane, Mompati and Essex, Max and Gaolethe, Tendani and Petlo, Chipo and Lockman, Shahin and Holmes, Lewis B. and Makhema, Joseph and Shapiro, Roger L.},
