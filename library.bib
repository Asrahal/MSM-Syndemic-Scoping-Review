  title = {Syndemic Production and Sexual Compulsivity/Hypersexuality in Highly Sexually Active Gay and Bisexual Men: Further Evidence for a Three Group Conceptualization},
  author = {Parsons, Jeffrey T and Rendina, H Jonathon and Moody, Raymond L and Ventuneac, Ana and Grov, Christian},
